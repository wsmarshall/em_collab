adj_list = []
for t in range(len(str_prdc7k4)): 
    current_vert = []
    for x in range(len(str_prdc7k4[t])): 
        if str_prdc7k4[t][x] == 1:
            current_vert.append(x)
    adj_list.append(current_vert)
    

print(adj_list)    
